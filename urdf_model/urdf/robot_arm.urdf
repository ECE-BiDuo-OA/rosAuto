<?xml version="1.0"?>
<robot name="visual">
    <material name="blue">
        <color rgba="0 0 .8 1"/>
    </material>
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>
    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>
    <link name="base_link">
        <visual>
            <geometry>
                <cylinder length="0.05" radius="0.2"/>
            </geometry>
            <material name="blue"/>
            <origin xyz="0 0 0"/>
        </visual>
    </link>
    <joint name="base_cyl1" type="continuous">
        <parent link="base_link"/>
        <child link="cyl1"/>
        <axis xyz="0 0 1"/>
        <origin xyz="0 0 0"/>
    </joint>
    <link name="cyl1">
        <visual>
            <geometry>
                <cylinder length="0.5" radius="0.05"/>
            </geometry>
            <material name="white"/>
        	<origin xyz="0 0 0.25"/>
        </visual>
    </link>
	<joint name="cyl1_cyl2" type="continuous">
        <parent link="cyl1"/>
        <child link="cyl2"/>
        <axis xyz="0 1 0"/>
        <origin xyz="0 -0.1 0.5"/>
    </joint>
    <link name="cyl2">
        <visual>
            <geometry>
                <cylinder length="0.4" radius="0.05"/>
            </geometry>
            <material name="blue"/>
        	<origin rpy="0 1.57075 0" xyz="0.2 0 0"/>
        </visual>
    </link>
    <joint name="cyl2_cyl3" type="continuous">
        <axis xyz="0 1 0"/>
        <parent link="cyl2"/>
        <child link="cyl3"/>
        <origin xyz="0.35 0.1 0"/>
    </joint>
    <link name="cyl3">
        <visual>
            <geometry>
                <cylinder length="0.4" radius="0.05"/>
            </geometry>
            <material name="white"/>
            <origin rpy="0 1.57075 0" xyz="0.15 0 0"/>
        </visual>
    </link>
    <joint name="cyl3_box" type="continuous">
        <axis xyz="1 0 0"/>
        <parent link="cyl3"/>
        <child link="box"/>
        <origin xyz="0.35 0 0"/>
    </joint>
    <link name="box">
        <visual>
            <geometry>
                <box size="0.05 0.05 0.05"/>
            </geometry>
            <material name="blue"/>
            <origin rpy="0 0 0" xyz="0.025 0 0"/>
        </visual>
    </link>    
</robot>
